<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developer.wctmnotes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wctm"
        android:label="@string/branch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubjectsActivity"
            android:label="@string/subjects"
            android:icon="@drawable/wctm"/>
        <activity android:name=".SemesterActivity"
            android:label="@string/semester"
            android:icon="@drawable/wctm"/>
        <activity android:name=".TeachersActivity"
            android:label="@string/teachers"
            android:icon="@drawable/wctm"/>
    </application>

</manifest>